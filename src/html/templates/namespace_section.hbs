<div class="namespaceSection">
  {{~#each this~}}
    <div id="{{this.id}}" class="namespaceItem" {{#if this.deprecated}}aria-label="deprecated"{{/if}}>
	    {{~> doc_node_kind_icon this.doc_node_kind_ctx ~}}

	    <div class="namespaceItemContent">
		    <a href="{{this.href}}" title="{{this.name}}">
			    <span>{{~this.name~}}</span>
          {{~#if this.ty~}}
            <span>
              {{{~this.ty~}}} {{! typedef rendering }}
            </span>
          {{~/if~}}
      </a>

	      <div class="namespaceItemContentDoc">
		      {{~#if this.docs~}}
			      {{{~this.docs~}}} {{! markdown rendering }}
		      {{~else~}}
			      <span class="italic">No documentation available</span>
		      {{~/if~}}
	      </div>

        {{~#if this.subitems~}}
          <ul class="namespaceItemContentSubItems">
            {{~#each this.subitems~}}
              <li>
                <a href="{{this.href}}">
                  <span>{{this.title}}</span>
                  {{~#if this.ty~}}
                    <span>
                      {{{~this.ty~}}} {{! typedef rendering }}
                    </span>
                  {{~/if~}}
                </a>

                <div class="namespaceItemContentDoc">
                  {{~#if this.docs~}}
                    {{{~this.docs~}}} {{! markdown rendering }}
                  {{~else~}}
                    <span class="italic">No documentation available</span>
                  {{~/if~}}
                </div>
              </li>
            {{~/each~}}
          </ul>
        {{~/if~}}
      </div>
    </div>
  {{~/each~}}
</div>
