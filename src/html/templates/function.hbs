{{~#each overloads_ctx~}}
  <style scoped>
    {{~this.additional_css~}}
  </style>
  <input type="radio" name="{{this.function_id}}" id="{{this.overload_id}}" {{this.html_attrs}} />
{{~/each~}}

<div class="space-y-2">
  {{~#each overloads_ctx~}}
    <label for="{{this.overload_id}}" class="space-y-1 block px-4 py-2.5 rounded-lg border border-stone-300 cursor-pointer hover:bg-stone-100">
      {{~#if (ne this.deprecated null)~}}
        <div>
          <div class="py-1 text-red-500 flex gap-1 items-center">
          <span class="font-semibold leading-6">
            Deprecated
          </span>
          </div>

          {{~#if (ne this.deprecated "")~}}
            <div class="ml-1 pl-2 border-l-4 border-red-300">
              {{this.deprecated}}
            </div>
          {{~/if~}}
        </div>
      {{~/if~}}
      <code class="text-sm">
        <span class="font-bold">{{this.name}}</span><span class="font-medium">{{this.summary}}</span>
      </code>
      {{~#if this.summary_doc~}}
        <div class="markdown_border">
          {{~this.summary_doc~}}
        </div>
      {{~/if~}}
    </label>
  {{~/each~}}
</div>

<div class="mt-3">
  {{~#each functions~}}
    {{~> symbol_content this ~}}
  {{~/each~}}
</div>
